{
  "plugins": [
    "transform-object-rest-spread",
    "transform-flow-strip-types",
  ],
  "env": {
    "lib": {
      "plugins": [
        ["transform-runtime", { "polyfill": false }]
      ],
      "presets": [
        ["env", {
          "targets": {
            "node": 6
          },
        }]
      ]
    },
    "es": {
    },
    "node8": {
      "plugins": [
        ["transform-runtime", { "polyfill": false }]
      ],
      "presets": [
        ["env", {
          "targets": {
            "node": "8.0.0"
          },
          "loose": true,
          "modules": "commonjs"
        }]
      ]
    },
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "node": "current"
          },
        }]
      ]
    }
  }
}
