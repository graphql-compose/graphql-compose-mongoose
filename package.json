{
  "name": "graphql-compose-mongoose",
  "version": "0.0.4",
  "description": "Plugin for `graphql-compose` which derive a graphql type from a mongoose model.",
  "files": [
    "es",
    "lib"
  ],
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodkz/graphql-compose-mongoose.git"
  },
  "keywords": [
    "graphql",
    "compose",
    "mongoose",
    "mongodb"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodkz/graphql-compose-mongoose/issues"
  },
  "homepage": "https://github.com/nodkz/graphql-compose-mongoose",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "graphql-compose": ">=0.0.2",
    "mongoose": "^4.4.20",
    "object-path": "^0.9.2"
  },
  "peerDependencies": {
    "graphql": "^0.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-es2015-loose-native-modules": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-spies": "^0.7.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.27.0",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.2",
    "sane": "^1.3.4"
  },
  "scripts": {
    "build": "npm run build-cjs && npm run build-es",
    "build-cjs": "rimraf lib && BABEL_ENV=cjs babel src --ignore __tests__,__mocks__ -d lib && cp src/definition.js lib/definition.js",
    "build-es": "rimraf es && BABEL_ENV=es babel src --ignore __tests__,__mocks__ -d es && cp src/definition.js es/definition.js",
    "lint": "eslint src test *.js",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register --reporter dot --require ./resources/mocha-bootload src/**/__tests__/**/*-test.js",
    "watch": "babel-node ./resources/watch.js"
  }
}
